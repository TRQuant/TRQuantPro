import schedule
import time

def start_daily_pipeline(
    pipeline: FactorPipeline,
    run_time: str = "18:00",  # é»˜è®¤æ”¶ç›˜åè¿è¡Œ
    stock_pool: str = "all_a"
):
    """
    å¯åŠ¨æ¯æ—¥å®šæ—¶æµæ°´çº¿
    
