        logger.info(f"å¼€å§‹æ¯æ—¥å› å­è®¡ç®—æµæ°´çº¿: {datetime.now()}")
        try:
            result = pipeline.run_pipeline()
            logger.info(f"æ¯æ—¥æµæ°´çº¿å®Œæˆ: {result}")
        except Exception as e:
            logger.error(f"æ¯æ—¥æµæ°´çº¿å¤±è´¥: {e}")
    
    # é…ç½®å®šæ—¶ä»»åŠ¡
    schedule.every().day.at(run_time).do(run_daily)
    
